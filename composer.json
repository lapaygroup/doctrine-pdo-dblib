{
    "name": "lapaygroup/doctrine-pdo-dblib",
    "type": "library",
    "description": "Doctrine driver for support of the pdo_dblib (MSSQL)",
    "keywords": ["pdo_dblib","pdo","dblib","mssql","doctrine", "doctrine-dblib"],
    "license": "MIT",
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/lapaygroup/doctrine-pdo-dblib/issues",
        "source": "https://github.com/lapaygroup/doctrine-pdo-dblib"
    },
    "authors": [
        {
            "name": "Nikolay Lapay",
            "email": "nl@wildtuna.pro"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-pdo": "*",
        "ext-iconv": "*",
        "doctrine/dbal": ">=2.1.0"
    },
    "autoload": {
        "psr-0": {
            "LapayGroup\\DoctrinePdoDblib": "src/"
        },
        "psr-4": {
            "LapayGroup\\DoctrinePdoDblib\\": "src/"
        }
    }
}
